{
  "name": "cerpus/coreclient",
  "description": "Client for communication with Core",
  "keywords": [
    "Cerpus Core API Client",
    "laravel",
    "lumen"
  ],
  "require": {
    "illuminate/http": "^6.0",
    "cerpus/cerpushelper": "^1.1",
    "ext-json": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.0",
    "illuminate/validation": "^6.0"
  },
  "authors": [
    {
      "name": "Thomas Horn Sivertsen",
      "email": "thomass@cerpus.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cerpus\\CoreClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cerpus\\CoreClientTests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.cerpus.net/"
    },
    {
      "type": "composer",
      "url": "https://composer-3rdparty.cerpus.net/"
    }
  ]
}
