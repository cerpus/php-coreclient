{
  "name": "cerpus/coreclient",
  "description": "Client for communication with Core",
  "type": "library",
  "keywords": [
    "Cerpus Core API Client",
    "laravel",
    "lumen"
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "illuminate/http": "*",
    "cerpus/cerpushelper": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.0",
    "illuminate/validation": "^6.0"
  },
  "authors": [
    {
      "name": "Thomas Horn Sivertsen",
      "email": "thomass@cerpus.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cerpus\\CoreClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cerpus\\CoreClientTests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.cerpus.net/"
    },
    {
      "type": "composer",
      "url": "https://composer-3rdparty.cerpus.net/"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Cerpus\\CoreClient\\Providers\\CoreClientServiceProvider"
      ],
      "aliases": {
        "CoreClient": "Cerpus\\CoreClient\\CoreClient"
      }
    }
  }
}
